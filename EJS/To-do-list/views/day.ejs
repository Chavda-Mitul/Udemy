<%- include('header'); -%>
    <div class="box" id="heading">
        <H1>
            <%= Tittle %>
        </H1>
    </div>

    <div class="box">
        <% for(let i=0;i<newItem.length;i++) { %>
            <div class="item">
                <input type="checkbox">
                <p>
                    <%= newItem[i] %>
                </p>
            </div>
            <%}%>
                <form action="/" method="post" class="item">
                    <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
                    <button type="submit" name="list" value="<%= Tittle %>">+</button>
                </form>
    </div>
    <%- include('fotter'); -%>